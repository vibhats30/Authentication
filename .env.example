# Database Configuration
DB_USERNAME=postgres
DB_PASSWORD=your_password_here

# JWT Configuration (generate a strong random key for production)
JWT_SECRET=your-256-bit-secret-key-here

# Google OAuth2
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Facebook OAuth2
FACEBOOK_CLIENT_ID=your-facebook-app-id
FACEBOOK_CLIENT_SECRET=your-facebook-app-secret

# GitHub OAuth2
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Twitter OAuth2
TWITTER_CLIENT_ID=your-twitter-client-id
TWITTER_CLIENT_SECRET=your-twitter-client-secret

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Logging Configuration
# Set log levels for different components (DEBUG, INFO, WARN, ERROR)
# Use INFO for production, DEBUG for development

# Overall application log level
LOG_LEVEL=INFO

# Component-specific log levels
LOG_LEVEL_CONTROLLER=INFO
LOG_LEVEL_SERVICE=INFO
LOG_LEVEL_SECURITY=INFO
LOG_LEVEL_FILTER=INFO

# Spring Security OAuth2 logging
LOG_LEVEL_SPRING_SECURITY=INFO
LOG_LEVEL_OAUTH2=INFO

# Database query logging (set to DEBUG to see SQL, TRACE to see parameters)
LOG_LEVEL_SQL=DEBUG
LOG_LEVEL_SQL_PARAMS=TRACE
